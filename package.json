{
  "name": "create-fetch",
  "version": "1.2.0",
  "description": "Utilities for custom fetch",
  "repository": "ambar/create-fetch",
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.js",
  "files": [
    "es",
    "lib",
    "src"
  ],
  "keywords": [
    "fetch",
    "http",
    "compose",
    "middleware"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prebuild": "rm -rf lib es dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --notify",
    "prepare": "npm run test && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "jest": "^23.5.0",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.1.0-0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-terser": "^3.0.0",
    "standard-version": "^4.4.0"
  }
}
